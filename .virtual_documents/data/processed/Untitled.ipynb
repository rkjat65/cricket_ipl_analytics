import pandas as pd


df = pd.read_csv('ipl_2021_matches.csv')


df_clean = df.drop_duplicates(subset=['match_date', 'team1', 'team2', 'venue'], keep='first')
df_clean.to_csv('ipl_2021_matches.csv', index=False)
print(f"2021: {len(df)} â†’ {len(df_clean)} matches")


